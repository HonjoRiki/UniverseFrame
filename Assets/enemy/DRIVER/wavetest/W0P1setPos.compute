#version 450
#pragma kernel main

GLSLPROGRAM

layout(std430, binding = 0)buffer Result { vec4 results[]; };

layout(local_size_x = 1, local_size_y = 1, local_size_z = 1) in;

void main()
{
	float interval=10.0;
	results[1+int(gl_WorkGroupID.x)*2] = vec4(interval*float(gl_WorkGroupID.x), 0.0, 0.0, 0.0);
}

ENDGLSL
